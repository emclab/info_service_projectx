<% if @js_erb_code %>
  <%= render inline: ERB.new(@js_erb_code).result(binding) %>
  
<% else %>
<% if params[:project][:field_changed] == 'customer_id' && params[:project][:customer_name_autocomplete].present? %>
  
    <% customer = InfoServiceProjectx.customer_class.find_by_name(params[:project][:customer_name_autocomplete].strip)%>
    <% if customer %>
      $('#project_customer_id').val("<%=customer.id%>"); 
    <% else %>
      $('#project_customer_id').val(""); 
    <% end %> 
<% elsif params[:project][:customer_name_autocomplete].blank? %>
  $('#project_customer_id').val(""); 
<% end %>


<% end %>